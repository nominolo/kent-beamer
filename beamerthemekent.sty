\mode<presentation>

\useoutertheme{kent}

\definecolor{kentblue}{cmyk}{1.0,0.58,0,0.21}
\definecolor{kentpink}{cmyk}{0,1.0,0.13,0.17}

\definecolor{gray20}{rgb}{0.8,0.8,0.8}

%% Baige colour set
\definecolor{colourset4col1dark}{cmyk}{0,0.07,0.39,0.17}
\definecolor{colourset4col1light}{cmyk}{0,0.02,0.08,0.1}

%% Blue/Cyan colour set
\definecolor{colourset5col1dark}{cmyk}{1.0,0.1,0,0.28}
